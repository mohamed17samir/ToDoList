@model IEnumerable<Todo>

<div class="main-container">
	<div class="cards">
		@foreach (var item in Model)
		{
			<div class="card card-1">
				<h1 class="title">@item.TaskName</h1>
				<a asp-action="Delete" asp-route-id="@item.Id" class="card__icon"><img class="iii" src="~/trash.png" /></a>
				<h3 class="card__title">@item.Ttask</h3>
				<p class="card__apply">
					<a class="card__link" asp-controller="Home" asp-action="Edit" asp-route-id="@item.Id">
						<img class="eee" src="~/icons8-edit-100.png" /><span class="sp">Edit</span></a>
				</p>
			</div>
		}
	</div>
</div>