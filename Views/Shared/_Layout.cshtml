<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Done</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/all.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Done.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
            <main class="main">
  <aside class="sidebar">
    <nav class="nav">
      <ul>
        <li class="active"><a asp-controller="Home" asp-action="Index">Home</a></li>
        <li class="active"><a asp-controller="Home" asp-action="Note">Notes</a></li>
        <li><a asp-controller="Home" asp-action="About">About Me</a></li>
     <partial name="_LoginPartial" />
      </ul>
    </nav>
  </aside>
        </main>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
